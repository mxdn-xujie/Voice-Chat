# 应用配置
PORT=3500
NODE_ENV=production

# MongoDB配置
MONGODB_URI=mongodb://mongodb:27017/voicechat

# 安全配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# 开发配置
DEV_MODE=false
ENABLE_LOGS=true
API_TIMEOUT=30000
MAX_UPLOAD_SIZE=10mb

# WebSocket配置
WS_HEARTBEAT_INTERVAL=30000
WS_RECONNECT_ATTEMPTS=5

# 语音配置
VOICE_QUALITY=high
MAX_VOICE_DURATION=300000  # 5分钟